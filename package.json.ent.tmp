{
  "name": "ent-practice-test",
  "version": "1.0.0",
  "homepage": "https://ent-practice-app.web.app",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "serve:web": "npx serve dist",
    "predeploy": "npm run build:web && touch dist/.nojekyll",
    "deploy": "gh-pages -d dist",
    "deploy:firebase": "expo export --platform web && rm -rf dist/admin && cp -R admin dist/admin && rm -rf dist/consents && cp -R public/consents dist/consents && firebase deploy",
    "deploy:firebase:hosting": "firebase deploy --only hosting",
    "deploy:firebase:rules": "firebase deploy --only firestore:rules",
    "deploy:firebase:indexes": "firebase deploy --only firestore:indexes",
    "test": "jest"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/virtualized-lists": "^0.83.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@types/react": "~19.1.10",
    "expo": "~54.0.32",
    "expo-apple-authentication": "~8.0.8",
    "expo-application": "~7.0.8",
    "expo-auth-session": "~7.0.10",
    "expo-crypto": "~15.0.8",
    "expo-font": "~14.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.8.0",
    "marked": "^12.0.2",
    "punycode": "^2.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "ts-fsrs": "^4.1.0",
    "typescript": "~5.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "firebase-admin": "^12.7.0",
    "firebase-tools": "^15.4.0",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.0"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": true
}
